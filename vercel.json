{
  "buildCommand": "prisma generate && next build",
  "framework": "nextjs",
  "outputDirectory": ".next",
  "regions": ["arn1"],
  "functions": {
    "app/api/**/*.ts": {
      "maxDuration": 30
    }
  },
  "build": {
    "env": {
      "NEXT_PUBLIC_APP_URL": "https://tma-subscription.vercel.app"
    }
  },
  "alias": [
    {
      "deployment": "production",
      "alias": "tma-subscription.vercel.app"
    }
  ]
}